{
  "expo": {
    "name": "Expose Me",
    "slug": "expose-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": [
      {
        "asset": "./assets/fonts/b.otf",
        "family": "IconFont"
      }
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.socketNull.ExposeMe",
      "icon": "./assets/images/adaptive-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ff73b7"
      },
      "package": "com.socketNull.ExposeMe"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6ab88ec9-1d9d-4f34-86be-b7ab1b3492a1"
      }
    },
    "owner": "rivotx"
  }
}
